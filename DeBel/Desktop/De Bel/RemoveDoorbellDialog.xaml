<Window x:Class="De_Bel.RemoveDoorbellDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:De_Bel"
        mc:Ignorable="d"
        Title="Remove Doorbell" Height="300" Width="300" ContentRendered="Window_ContentRendered">
    <Grid>
        <Button x:Name="btnRemove" Content="Remove" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="75" Click="btnRemove_Click"/>
        <Button x:Name="btnClose" Content="Close" Margin="0,0,10,10" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="btnClose_Click"/>
        <ListBox x:Name="lboxDoorbells" Margin="0,0,0,35"/>
    </Grid>
</Window>
